<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Button.purple_200" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:backgroundTint">@color/purple_200</item>
        <item name="android:backgroundTintMode">src_over</item>
    </style>

    <style name="ButtonBack">
        <item name="android:backgroundTint">@color/bg</item>
        <item name="android:backgroundTintMode">src_over</item>
        <item name="android:src">@drawable/ic_baseline_arrow_back_24</item>
    </style>

    <style name="SignTitle">
        <item name="android:textColor">@color/purple_200</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>